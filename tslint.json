{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["**/*.js"]
  },
  "rules": {
    "interface-name": [false, "never-prefix"],
    "no-empty-interface": [false],
    "object-literal-sort-keys": false,
    "strict-type-predicates": false
  }
}
